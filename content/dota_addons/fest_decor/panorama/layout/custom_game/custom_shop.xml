		 <include src="file://{resources}/styles/custom_game/barebones_hud_base.css" />
	</styles>
    <scripts>
        <include src="file://{resources}/scripts/custom_game/custom_shop.js" />
	 </scripts>
	
    <Panel hittest="false" class="custom_shop_holder" oncancel="hideShop()">
	
		
	<Panel hittest="false" class="GageHolder" >
			<Image id="Tree" src="file://{images}/custom_game/tree_gage.png" />
			<Image id="gage_bkg" src="file://{images}/custom_game/gage_bkg.png" />

</Panel>
<Panel hittest ="false" class="teamholder">
			<Image id="team_mitten" src="file://{images}/custom_game/mitten.png" />
			<Image id="team_stocking" src="file://{images}/custom_game/stocking.png" />
			</Panel>
			
<Panel hittest="false" class="AbilityHolder" >
			<Image id="presentclosed" src="file://{images}/custom_game/presentclosed.png" />
			<Image id="presentopen" src="file://{images}/custom_game/presentopen.png" />
			<Image id="rocketskate" src="file://{images}/custom_game/rocketskate.png" />
			<Image id="magnet" src="file://{images}/custom_game/magnet.png" />
			<Image id="snowball" src="file://{images}/custom_game/snowball.png" />
			
</Panel>

<Panel hittest="false" class="MinimapFrameHolder" >
			<Image id="MinimapFrame" src="file://{images}/custom_game/candycaneminimap.png" />
			
</Panel>

<Image id="TimerFrame" src="file://{images}/custom_game/clockholder.png" />

<Panel hittest="false" id="TimerHolder" >
			<Label id = "TimeLeft" class="Timer" text="00:00" />	
			
</Panel>

	
	
    <Panel hittest="false" class="BarebonesTopNotifications">
      <Panel hittest="false" id="TopNotifications">
      </Panel>
    </Panel>
    <Panel hittest="false" class="BarebonesBottomNotifications">
      <Panel hittest="false" id="BottomNotifications">
      </Panel>
    </Panel>
	<Panel hittest="true" class="BellHolder" >
			<Image id="bell" src="file://{images}/custom_game/bell2.png" />
			<Image id="bellgrey" src="file://{images}/custom_game/bell2grey.png" />
			
</Panel>


    </Panel>
</root>