<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/custom_game/overthrow_game_info.css" />
	</styles>
	<Panel class="OverthrowGameInfo">
		
			<Image id="Logo" src="file://{images}/custom_game/logo.png" />
		
		<Label id="ObjectiveHeader" class="Header" html="true" text="#objective_header_lang" />
		<Label id="BriefDescription" text="#brief_description_lang"/>
		<Label id="HowToPlayHeader" class="Header" html="true" text="#HowToPlayHeader_lang" />
		<Label id="HowToPlay" text="#HowToPlay_lang"/>
		<Label id="PointsHeader" class="Header" html="true" text="#PointsHeader_lang" />
		<Label id="Points" text="#Points_lang"/>
		
			<Panel id ="PointsHolder">
			<Image id="Bauble" src="file://{images}/custom_game/baubleinfo.png" />
			<Label id="BaublePoints" text="#BubblePoints_lang"/>
			
			<Image id="CandyCane" src="file://{images}/custom_game/candycaneinfo.png" />
			<Label id="CandyCanePoints" text="#CandyCanePoints_lang"/>
			
			<Image id="Star" src="file://{images}/custom_game/starinfo.png" />
			<Label id="StarPoints" text="#StarPoints_lang"/>
			
			<Image id="Angel" src="file://{images}/custom_game/angelinfo.png" />
			<Label id="AngelPoints" text="#AngelPoints_lang"/>
			</Panel>

		<Label id="AbilitiesHeader" class="Header" html="true" text="#AbilitiesHeader" />
		<Label id="Abilities" text="#Abilities"/>
		<Panel id ="AbilitiesHolder">
			<Image id="Skate" src="file://{images}/custom_game/rocketskate.png" />
			<Label id="SkateInfo" text="#SkateInfo_lang"/>
			
			<Image id="Magnet" src="file://{images}/custom_game/magnet.png" />
			<Label id="MagnetInfo" text="#MagnetInfo_lang"/>
			
			<Image id="Snowball" src="file://{images}/custom_game/snowball.png" />
			<Label id="SnowballInfo" text="#SnowballInfo_lang"/>
			</Panel>
		
		
	</Panel>
</root>

